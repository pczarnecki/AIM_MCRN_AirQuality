s(grocery, k = 5)+s(pm25, k = 5) +s(med_temp, k=5) + s(med_hum, k=5), data = italy_25, family = gaussian)
plot(italy_25gam, scale=0,se=2, shade=TRUE,pages=1, scheme = 2,main = "23 Cities in Italy")
summary(italy_25gam)
gam.check(italy_25gam)
